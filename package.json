  },
  "homepage": "https://github.com/Better-er-Help/project3#readme",
  "devDependencies": {
    "concurrently": "^6.0.0",
    "if-env": "^1.0.4",
    "nodemon": "^2.0.7"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "dot-env": "0.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.2",
    "path": "^0.12.7",
    "router": "^1.3.5"
  }
